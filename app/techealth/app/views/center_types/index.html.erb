<p id="notice"><%= notice %></p>

<h1>Center Types</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @center_types.each do |center_type| %>
      <tr>
        <td><%= center_type.type %></td>
        <td><%= link_to 'Show', center_type %></td>
        <td><%= link_to 'Edit', edit_center_type_path(center_type) %></td>
        <td><%= link_to 'Destroy', center_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Center Type', new_center_type_path %>
