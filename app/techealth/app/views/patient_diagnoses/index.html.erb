<p id="notice"><%= notice %></p>

<h1>Patient Diagnoses</h1>

<table>
  <thead>
    <tr>
      <th>Id patient</th>
      <th>Diagnose</th>
      <th>Level</th>
      <th>Observation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @patient_diagnoses.each do |patient_diagnosis| %>
      <tr>
        <td><%= patient_diagnosis.id_patient %></td>
        <td><%= patient_diagnosis.diagnose_id %></td>
        <td><%= patient_diagnosis.level %></td>
        <td><%= patient_diagnosis.observation %></td>
        <td><%= link_to 'Show', patient_diagnosis %></td>
        <td><%= link_to 'Edit', edit_patient_diagnosis_path(patient_diagnosis) %></td>
        <td><%= link_to 'Destroy', patient_diagnosis, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Patient Diagnosis', new_patient_diagnosis_path %>
